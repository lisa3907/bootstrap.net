@inherits BootStrapComponentBase

<DynamicElement Attributes="@UnknownParameters" TagName="label" class="@classname">
    @ChildContent
</DynamicElement>

@functions {
    string classname
    {
        get
        {
            return Class;
        }
    }
    [Parameter] public string Class { get; set; }
    [Parameter] public RenderFragment ChildContent { get; set; }
}